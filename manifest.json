{
    "name": "My Extension",
    "description": "A simple extension that allows you to write and save notes.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "storage"
    ]
    {
        "name": "My Extension",
        "version": "1.0",
        "manifest_version": 3,
        "permissions": [
          "storage"
        ],
        "content_scripts": [
          {
            "matches": [
              "<all_urls>"
            ],
            "js": [
              "popup.js"
            ]
          }
        ],
        "action": {
          "default_popup": "popup.html"
        }
      }
      
  }
  